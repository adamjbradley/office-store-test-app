<OfficeApp xsi:type="MailApp" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://schemas.microsoft.com/office/appforoffice/1.0">
  <Id>{502a5942-52ea-4293-83b3-0ec4f2d6b4b8}</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>[Provider name]</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="Mail Read Sample" />
  <Description DefaultValue="Get the subject and from fields from an email message" />
  <Capabilities>
    <Capability Name="Mailbox" />
  </Capabilities>
  <DesktopSettings>
    <!--To publish to the Add-in Store, enter the URL that this app will be hosted-->
    <SourceLocation DefaultValue="App/index.html" />
    <RequestedHeight>250</RequestedHeight>
  </DesktopSettings>
  <TabletSettings>
    <!--To publish to the Add-in Store, enter the URL that this app will be hosted-->
    <SourceLocation DefaultValue="App/index.html" />
    <RequestedHeight>250</RequestedHeight>
  </TabletSettings>
  <PhoneSettings>
    <!--To publish to the Add-in Store, enter the URL that this app will be hosted-->
    <SourceLocation DefaultValue="App/index.html" />
  </PhoneSettings>
  <Permissions>ReadItem</Permissions>
  <Rule xsi:type="RuleCollection" Mode="And">
    <!-- To add more complex rules, add additional rule elements -->
    <Rule xsi:type="ItemIs" ItemType="Message" />
    <!-- E.g. To activate when a message contains an address -->
    <!-- <Rule xsi:type="ItemHasKnownEntity" EntityType="Address" /> -->
  </Rule>
</OfficeApp>